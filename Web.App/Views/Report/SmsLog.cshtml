@model IEnumerable<Web.Models.Tables.SmsDetails>

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h2 class="panel-title">Results</h2>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-condensed">
                        <thead>
                            <tr>
                                <th>
                                    Id
                                </th>
                                <th>
                                    Api Response
                                </th>
                                <th>
                                    Message
                                </th>
                                <th>
                                    Date sent
                                </th>
                                <th>
                                    Sender
                                </th>
                                <th>
                                    Student
                                </th>
                                <th>
                                    Section
                                </th>
                                <th>
                                    Room
                                </th>
                                <th>
                                    Guardian
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var log in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.SmsDetailsId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Status)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Message)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.DateSent)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.AspNetUsers.UserName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Student.FullName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Student.Section.SectionName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Student.Room.RoomDetails)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Guardian.FullName)
                                </td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>