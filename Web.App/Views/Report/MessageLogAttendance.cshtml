@model IEnumerable<Web.Models.Tables.AttendanceLog>


<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h2 class="panel-title">Results</h2>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-condensed">
                        <thead>
                            <tr>
                                <th>
                                    Id
                                </th>
                                <th>
                                    Api Response
                                </th>
                                <th>
                                    Student
                                </th>
                                <th>
                                    Address
                                </th>
                                <th>
                                    Section
                                </th>
                                <th>
                                    Room
                                </th>
                                <th>
                                    Time Type
                                </th>
                                <th>
                                    Time value
                                </th>
                                <th>
                                    Date
                                </th>
                                <th>
                                    Guardian
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var log in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.AttendanceLogId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.ApiResponse)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Student.FullName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Student.Address)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Student.Section.SectionName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Student.Room.RoomDetails)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.AttendanceDetails.TimeType.TimeTypeName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.AttendanceDetails.Time)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.AttendanceDetails.Date)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => log.Guardian.FullName)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
